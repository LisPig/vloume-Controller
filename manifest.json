{
  "name": "VolumeController",
  "version": "0.0.2",
  "manifest_version": 3,
  
  "description": "",
  "permissions": [
    "tabCapture",
    "activeTab",
    "storage",
    "scripting",
    "tabs"
    
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "images/logo125.png",
    "32": "images/logo125.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo125.png",
      "32": "images/logo125.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  },
  "background":{
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": ["*://*.chrome/*"],
      "js": ["content_script.js"],
      
      "run_at": "document_end"
    }
  ]
}